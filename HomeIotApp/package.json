    "start": "react-native webpack-start --port 9001",
    "bundle": "react-native webpack-bundle",
    "test": "jest",
    "format": "prettier --write .",
    "postinstall": "husky install",
    "run:android": "ENVFILE=.env.development npx react-native run-android",
